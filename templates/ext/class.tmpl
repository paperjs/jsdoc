
<div class="body-wrap">
    <div class="top-tools">
        <a class="inner-link" href="#{[values.get("alias")]}-props"><img src="resources/s.gif" class="item-icon icon-prop">Properties</a>
        <a class="inner-link" href="#{[values.get("alias")]}-methods"><img src="resources/s.gif" class="item-icon icon-method">Methods</a>
        <a class="inner-link" href="#{[values.get("alias")]}-events"><img src="resources/s.gif" class="item-icon icon-event">Events</a>
        <a class="inner-link" href="#{[values.get("alias")]}-configs"><img src="resources/s.gif" class="item-icon icon-config">Config Options</a>
        <a class="bookmark" href="NEED_TO_CREATE_DIRECT_LINK_HREF"><img src="resources/s.gif" class="item-icon icon-fav">Direct Link</a>
    </div>
    
    <div class="inheritance res-block">
        <pre class="res-block-inner">
    <a ext:cls="{[values.get("augments")]}" ext_member="" href="{[this.linker.toSymbol(values.get("augments")).toString(false)]}">{[values.get("augments")]}</a>
    <img src="resources/elbow-end.gif"/>{[values.get("alias")]}
        </pre>
    </div>
    <h1>Class {[values.get("alias")]}</h1>     
    <table cellspacing="0">
        <tr><td class="label">Package:</td><td class="hd-info">{[this.getPackageName(values.get("alias"))]}</td></tr>
        <tr><td class="label">Defined In:</td><td class="hd-info"><a ext:cls="{[values.get("alias")]}" ext:member="" href="{[this.linker.toSrc(values.get("srcFile")).toString(false)]}">{[JSDOC.Util.fileName(values.get("srcFile"))]}</a></td></tr>
        <tr><td class="label">Class:</td><td class="hd-info">{[this.getClassName(values.get("alias"))]}</td></tr>
        <tr><td class="label">Subclasses:</td><td class="hd-info">this could be figured out</td></tr>
        <tr><td class="label">Extends:</td><td class="hd-info"><a ext:cls="{[values.get("alias")]}" ext:member="" href="{[this.linker.toSymbol(values.get("augments")).toString(false)]}">{[values.get("augments")]}</a></td></tr>        
    </table>
    
    <div class="description">
        {[values.get("desc")]} 
    </div>
    
    <div class="hr"></div>
    <a id="{[values.get("alias")]}-configs"></a>
    <h2>Config Options</h2>
    <tpl if="!hasCfg()"><p>This class has no config options.</p></tpl>
    <table cellspacing="0" class="{[(values.hasCfg()) ? "member-table" : "x-hidden"]}">
        <tr>
            <th class="sig-header" colspan="2">Config Options</th>
            <th class="msource-header">Defined By</th>
        </tr>
        
        <tpl for="getCfg()">     
        <tr class="config-row expandable{[this.isInherited(values, parent) ? " inherited" : ""]}">
            <td class="micon"><a class="exi" href="#expand">&nbsp;</a></td>
            <td class="sig">
                <a id="Ext.data.DataProxy-listeners"></a>
                <b>{[values.get("name")]}</b> : {[values.get("type")]} 
                <div class="mdesc">
                    <div class="short">{[values.get("desc")]}</div>
                    <div class="long">
                        {[values.get("desc")]}
                    </div>
                </div>
            </td>
            <td class="msource">
                <tpl if="!this.isInherited(values, parent)">{[this.getClassName(values.get("memberof"))]}</tpl>
                <a class="{[!this.isInherited(values, parent) ? "x-hidden" : ""]}" ext:cls="{[values.get("memberof")]}" ext:member="{[values.get("alias")]}" href="{[this.linker.toSymbol(values.get("memberof")).toString(false)]}">{[this.getClassName(values.get("memberof"))]}</a>
            </td>
        </tr>
        </tpl>
        
    </table>
    <a id="{[values.get("alias")]}-props"></a>      
    
    <h2>Public Properties</h2> 
    <tpl if="!hasProperties()"><p>This class has no public properties.</p></tpl>        
    <table cellspacing="0" class="{[(values.hasProperties()) ? "member-table" : "x-hidden"]}">            
        <tr>
            <th class="sig-header" colspan="2">Property</th>
            <th class="msource-header">Defined By</th>
        </tr>
        
        <tpl for="getProperties()">
                    
        <tr class="property-row{[this.isInherited(values, parent) ? " inherited" : ""]}">
            <td class="micon">
                <a class="exi" href="#expand">&nbsp;</a>
            </td>
            <td class="sig">
                <a id="Ext.Component-disabled"></a>
                <b>{[values.get("name")]}</b> : {[values.get("type")]}            
                <div class="mdesc">   
                    {[values.get("desc")]}                        
                </div>
            </td>
            <td class="msource">                
                <tpl if="!this.isInherited(values, parent)">{[this.getClassName(values.get("memberof"))]}</tpl>
                <a class="{[!this.isInherited(values, parent) ? "x-hidden" : ""]}" ext:cls="{[values.get("memberof")]}" ext:member="{[values.get("alias")]}" href="{[this.linker.toSymbol(values.get("memberof")).toString(false)]}">{[this.getClassName(values.get("memberof"))]}</a>
            </td>
        </tr>
        
        </tpl>
        
        <!-- TODO: implement this alt row.  also notice non-linked "Defined by" when property is a part of this class.     
        <tr class="property-row alt">
            <td class="micon"><a class="exi" href="#expand">&nbsp;</a></td>
            <td class="sig">
                <a id="Ext.Component-hidden"></a>
                <b>hidden</b> : Boolean            
                <div class="mdesc">                            
                    True if this component is hidden. Read-only.                       
                </div>
            </td>
            <td class="msource">Component</td>
        </tr>
        -->
    </table>
                   
    <a id="{[values.get("alias")]}-methods"></a>
    <h2>Public Methods</h2>    
    <tpl if="!hasMethods()"><p>This class has no public methods</p></tpl>
    <table cellspacing="0" class="{[values.hasMethods() ? "member-table" : "x-hidden"]}">
        <tr>
            <th class="sig-header" colspan="2">Method</th>            
            <th class="msource-header">Defined By</th>
        </tr>
        
        <tpl for="getMethods()">        
        <tr class="method-row expandable{[this.isInherited(values, parent) ? " inherited" : ""]}">
            <td class="micon"><a class="exi" href="#expand">&nbsp;</a></td>
            <td class="sig">
                <a id="Ext.data.DataProxy-addEvents"></a>                
                <b>{[values.get("name")]}</b>(&nbsp;<code><tpl for="get('params')">{[(!values.isOptional) ? values.type + " " + values.name : "[" + values.type + " " + values.name + "]"]}<tpl if="xindex < xcount">, </tpl></tpl></code>&nbsp;) : {[(values.get("type") ? values.get("type") : "void")]}                                
                <div class="mdesc">
                    <div class="short">{[values.get("desc")]}</div>
                    <div class="long">
                        {[values.get("desc")]}    
                        <div class="mdetail-params">                            
                            <strong>Parameters:</strong>                            
                            <ul>
                            <tpl for="get('params')">
                                <li>
                                    <code>{name}</code> : {type}
                                    <div class="sub-desc">{[(values.isOptional==true) ? (optional) : ""]}&nbsp;{desc}</div>                                   
                                </li>
                            </tpl>
                            </ul>
                                                        
                            <strong>Returns:</strong>
                            <ul>
                                <li><code>{[(values.get("type")) ? values.get("type") : "void"]}</code></li>
                            </ul>                    
                        </div>
                    </div>                    
                </div>
            </td>
            <td class="msource">
                <tpl if="!this.isInherited(values, parent)">{[this.getClassName(values.get("memberof"))]}</tpl>
                <a class="{[!this.isInherited(values, parent) ? "x-hidden" : ""]}" ext:cls="{[values.get("memberof")]}" ext:member="{[values.get("alias")]}" href="{[this.linker.toSymbol(values.get("memberof")).toString(false)]}">{[this.getClassName(values.get("memberof"))]}</a>
            </td>
        </tr>
        </tpl>                                                  
    </table>
    
    <a id="{[values.get("alias")]}-events"></a>
    <h2>Public Events</h2>
    <tpl if="!hasEvents()"><p>This class has no events</p></tpl>
    <table cellspacing="0" class="{[(values.hasEvents()) ? "member-table" : "x-hidden"]}">        
        <tr>
            <th class="sig-header" colspan="2">Event</th>   
            <th class="msource-header">Defined By</th>
        </tr>
        <tpl for="getEvents()">
        <tr class="event-row expandable{[this.isInherited(values, parent) ? " inherited" : ""]}">
            <td class="micon"><a class="exi" href="#expand">&nbsp;</a></td>
            <td class="sig">
                <a id="Ext.data.DataProxy-beforeload"></a>
                <b>{[values.get("name")]}</b> : (&nbsp;<code><tpl for="get('params')">{[(!values.isOptional) ? values.type + " " + values.name : "[" + values.type + " " + values.name + "]"]}<tpl if="xindex < xcount">, </tpl></tpl></code>&nbsp;) 
            
                <div class="mdesc">   
                    <div class="short">{[values.get("desc")]}</div>
                    <div class="long">
                        {[values.get("desc")]}  
                        <div class="mdetail-params">
                            <strong style="font-weight:normal;">Listeners will be called with the following arguments:</strong>
                            <ul>
                            <tpl for="get('params')">
                                <li>
                                    <code>{name}</code> : {type}
                                    <div class="sub-desc">{desc}</div>                                   
                                </li>
                            </tpl>                                                            
                            </ul>   
                        </div>
                    </div>
                </div>
            </td>   
            <td class="msource">
                <tpl if="!this.isInherited(values, parent)">{[this.getClassName(values.get("memberof"))]}</tpl>
                <a class="{[!this.isInherited(values, parent) ? "x-hidden" : ""]}" ext:cls="{[values.get("memberof")]}" ext:member="{[values.get("alias")]}" href="{[this.linker.toSymbol(values.get("memberof")).toString(false)]}">{[this.getClassName(values.get("memberof"))]}</a>
            </td>                                 
        </tr>
        </tpl>            
    </table>            
</div>
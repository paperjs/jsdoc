<if test="defined(data.groupTitle)">
<h3>{+data.groupTitle+}</h3>
</if>
<div id="{+ data.id +}" class="member">
<div id="{+ data.id +}-link" class="member-link">
<a name="{+ data.id +}" href="#" onClick="return toggleMember('{+ data.id +}', false);"><tt><b>{+ data.name +}</b>{+ data.signature +}</tt></a>
</div>
<div id="{+ data.id +}-description" class="member-description hidden">
<div class="member-header">
<div class="member-title">
<div class="member-link">
<a href="#" onClick="return toggleMember('{+ data.id +}', false);"><tt><b>{+ data.name +}</b>{+ data.signature +}</tt></a>
</div>
</div>
<div class="member-close"><input type="button" value="Close" onClick="toggleMember('{+ data.id +}', false);"></div>
<div class="clear"></div>
</div>
<div class="member-text">
	{+ data.description +}
	{+ Render.parameters(data.symbol) +}
	{+ Render.returns(data.symbol) +}
	{+ Render.seeAlsos(data.symbol) +}
	{+ Render.examples(data.symbol) +}
</div>
</div>
</div>
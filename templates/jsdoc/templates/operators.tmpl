<div id="{+ data.id +}" class="member">
<div id="{+ data.id +}-link" class="member-link">
<a name="{+ data.id +}" href="#" onClick="return toggleMember('{+ data.id +}', false);"><tt>{+ data.title +}</tt></a>
</div>
<div id="{+ data.id +}-description" class="member-description hidden">
<for each="operator" in="data.operators">
{+ Render.operator(operator, data.id) +}
</for>
</div>
</div>

<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN"
        "http://www.w3.org/TR/html4/loose.dtd">
<html lang="en">
<head>
	<meta http-equiv="content-type" content="text/html; charset=utf-8">
	<title>All Classes</title>
	
	<style type="text/css">
		body {
			padding: 0;
			margin: 0;
		}
		
		h1 {
			font-family: arial, sans-serif;
			font-size: 16px;
			line-height: 20px;
			padding: 0;
			margin: 1em 0 0 .3em;
		}
		.classList
		{
			list-style-type: none;
			padding: 0;
			margin: 0 .6em 0 .6em;
			font-family: arial, sans-serif;
			font-size: 14px;
			line-height: 20px;
		}
		.classList li
		{
			padding: 0;
			margin: 0 0 .14em 0;
		}
	</style>
</head>
<body>

	<h1><a href="overview-summary.html" target="classFrame">All Classes</a></h1>
	
	<ul class="classList">
		<for each="thisClass" in="data">
		<li>{!
			if (thisClass.alias == "_global_") {
				output += "<i>"+new Link().toSymbol(thisClass.alias).target("classFrame")+"</i>";
			}
			else {
				output += new Link().toSymbol(thisClass.alias).target("classFrame");
			}
		!}</li>
		</for>
	</ul>

</body>
</html>

<h2>{+new Link().toFile("index.html").withText("Class Index")+}</h2>

<hr />
<ul class="classList">
	<for each="thisClass" in="data">
	<li>{!
		if (thisClass.get('alias') == "_global_") {
			output += "<i>"+new Link().toClass(thisClass.get('alias'))+"</i>";
		}
		else {
			output += new Link().toClass(thisClass.get('alias'));
		}
	!}</li>
	</for>
</ul>